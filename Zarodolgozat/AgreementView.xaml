<Window x:Class="Zarodolgozat.AgreementWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Zarodolgozat"
        mc:Ignorable="d"
        Title="AgreementWindow" Height="300" Width="567.097">
    <Grid>
        <DataGrid ItemsSource="{Binding AgreementList}" SelectedItem="{Binding SelectAgreement}" HorizontalAlignment="Left" Height="205" Margin="10,24,0,0" VerticalAlignment="Top" Width="220" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="test1" Binding="{Binding StartDate}" />
                <DataGridTextColumn Header="test2" Binding="{Binding Client.FirstName}" />
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="Új megállapodás" HorizontalAlignment="Left" Height="27" Margin="288,51,0,0" VerticalAlignment="Top" Width="86" Click="AddButton_Click"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Height="40" Margin="296,111,0,0" VerticalAlignment="Top" Width="118" Click="EditButton_Click"/>
    </Grid>
</Window>
